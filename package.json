  "version": "0.10.11",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "repository": "codenautas/self-explain",
  "license": "MIT",
  "main": "lib/self-explain.js",
  "files": ["lib", "dist"],
  "dependencies": {
    "escodegen": "~1.8.1",
    "esprima": "~3.1.1",
    "fs-extra": "~1.0.0",
    "fs-promise": "~1.0.0",
    
    "best-globals": "~0.7.0"
  },
  "devDependencies": {
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "expect.js": "~0.3.1",
    "express": "~4.14.0",
    "express-session": "~1.14.2",
    "express-useragent": "~1.0.4",
    "istanbul": "~0.4.5",
    "karma": "~1.3.0",
    "karma-chrome-launcher": "~2.0.0",
    "karma-coverage": "~1.1.1",
    "karma-coverage-html-index-reporter": "~1.0.2",
    "karma-expect": "~1.1.3",
    "karma-firefox-launcher": "~1.0.0",
    "karma-ie-launcher": "~1.0.0",
    "karma-mocha": "~1.2.0",
    "karma-phantomjs-launcher": "~1.0.2",
    "karma-safari-launcher": "~1.0.0",
    "mocha": "~3.1.2",
    "read-yaml-promise": "~1.0.2",
    "sinon": "~1.17.6",
    
    "extension-serve-static": "~0.1.6",
    "js-to-html": "~0.9.2",
    "require-bro": "~0.1.4",
    "tedede": "~0.2.5"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test-k": "node server/karma-server.js --karma karma.conf.js --single-run",
    "test-k-h": "node server/karma-server.js --karma karma.conf.js",
    "all": "npm test && npm run test-cov && qa-control . -v",
    "start": "node demo/demo.js"
  },
  "qa-control": {
    "package-version": "0.3.0",
    "coverage": 95,
    "run-in": "both",
    "stability": "extending",
    "type": "lib"
  }
}
