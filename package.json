{
  "name": "self-explain",
  "description": "self explained tools - starting with \"assert\"",
  "version": "0.10.1",
  "author": "Codenautas <codenautas@googlegroups.com>",
  "repository": "codenautas/self-explain",
  "license": "MIT",
  "main": "lib/self-explain.js",
  "files": ["lib", "dist"],
  "dependencies": {
    "escodegen": "~1.8.0",
    "esprima": "~2.7.2",
    "fs-extra": "~0.30.0",
    "fs-promise": "~0.5.0",
    
    "best-globals": "~0.4.1",
    "best-promise": "~0.2.4"
  },
  "devDependencies": {
    "body-parser": "~1.15.1",
    "cookie-parser": "~1.4.1",
    "expect.js": "~0.3.1",
    "express": "~4.13.4",
    "express-session": "~1.13.0",
    "express-useragent": "~0.2.4",
    "istanbul": "~0.4.3",
    "karma": "~0.13.22",
    "karma-chrome-launcher": "~1.0.1",
    "karma-coverage": "~1.0.0",
    "karma-coverage-html-index-reporter": "~1.0.2",
    "karma-expect": "~1.1.2",
    "karma-firefox-launcher": "~1.0.0",
    "karma-ie-launcher": "~1.0.0",
    "karma-mocha": "~1.0.1",
    "karma-phantomjs-launcher": "~1.0.0",
    "karma-safari-launcher": "~1.0.0",
    "karma-slimerjs-launcher": "~1.0.0",
    "lodash": "~4.12.0",
    "mocha": "~2.4.5",
    "read-yaml-promise": "~1.0.2",
    "sinon": "~1.17.4",
    
    "extension-serve-static": "~0.1.6",
    "js-to-html": "~0.9.0",
    "require-bro": "~0.1.0",
    "tedede": "~0.2.2"
  },
  "engines": {
    "node": ">= 0.12"
  },
  "scripts": {
    "test": "mocha --reporter spec --bail --check-leaks test/",
    "test-ci": "istanbul cover node_modules/mocha/bin/_mocha --report lcovonly -- --reporter spec --check-leaks test/",
    "test-cov": "istanbul cover node_modules/mocha/bin/_mocha -- --reporter dot --check-leaks test/",
    "test-k": "node server/karma-server.js --karma karma.conf.js --single-run",
    "test-k-h": "node server/karma-server.js --karma karma.conf.js",
    "start": "node demo/demo.js"
  },
  "jshintConfig": {
    "asi": false,
    "forin": true,
    "curly": true
  },
  "eslintConfig": {
    "env": {
      "node": false
    },
    "rules": {
      "strict": 0,
      "no-console": 1,
      "no-unused-vars": 1
    }
  },
  "qa-control": {
    "package-version": "0.2.0",
    "coverage": 95,
    "run-in": "both",
    "stability": "extending",
    "type": "lib"
  }
}
